        "@types/fast-json-patch": "^1.0.4",
        "@types/fossil-delta": "^1.0.0",
        "@types/mocha": "^2.2.44",
        "@types/msgpack-lite": "^0.1.1",
        "@types/reconnectingwebsocket": "^1.0.1",
        "benchmark": "^2.1.4",
        "chai": "^3.5.0",
        "mocha": "^5.1.1",
        "msgpack-lite": "^0.1.20",
        "node-localstorage": "^1.3.1",
        "nodemon": "^1.17.4",
        "ts-loader": "^2.3.7",
        "ts-node": "^6.0.3",
        "tslint": "^5.9.1",
